# status

git status
git log
#actualizar repo
git pull
git pull nombredelremote nombredelbranch
git pull origin master
#empujar cambios
git push
git push origin nombredelbranch

#listar branches
git branch -v

#crear branch
git checkout -b nombredelbranch

#cambiar de branch
git checkout nombredelbranch

#pushear al branch

git push origin nombredelbranch

#fusionar ramas
#primero entrar a la rama a fusionar
git checkout nombredelbranch
#luego fusionar con la otra rama, en este caso master
git merge master